<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Paperformat for consignment note report -->
        <record id="cust_paperformat_consignment_note" model="report.paperformat">
            <field name="name">Consignment Note Paperformat</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">149</field>
            <field name="page_width">210</field>
            <field name="orientation"></field>
            <field name="margin_top">7</field>
            <field name="margin_bottom">7</field>
            <field name="margin_left">14</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
        </record>
    </data>
</odoo>
